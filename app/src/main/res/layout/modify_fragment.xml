<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#72B0CC"
    tools:context=".fragments.Modify">

    <ImageView
        android:id="@+id/imageViewFirst"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:src="@drawable/ic_baseline_bedtime_24"
        app:layout_constraintBottom_toTopOf="@+id/editTextnameModifyCity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextnameModifyCity"
        android:layout_width="241dp"
        android:layout_height="50dp"
        android:background="@android:drawable/dialog_holo_light_frame"
        android:ems="10"
        android:hint="Modificar Ciudad"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.524" />

    <Button
        android:id="@+id/btnModify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Actualizar"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="@+id/editTextnameModifyCity"
        app:layout_constraintTop_toBottomOf="@+id/editTextnameModifyCity" />
</androidx.constraintlayout.widget.ConstraintLayout>